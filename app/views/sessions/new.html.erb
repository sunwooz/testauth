<%= image_tag("login.png", :class => "loginimg") %>
<div class="login">
<%= form_tag sessions_path do %>
  <ul>
    <li><%= text_field_tag :email, params[:email],:placeholder => "Your Email" %></li>

    <li><p id="passwordlabel">*Password</p></li>
    <li><%= password_field_tag :password %></li>
    <li><%= submit_tag "Log in", :class => "btn btn-large", :id => "loginbtn" %></li>
  </ul>
<% end %>
</div>
