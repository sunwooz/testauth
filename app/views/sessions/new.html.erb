
<h1 class="topic">Log In</h1>
<div class="login">
<%= form_tag sessions_path do %>
  <ul>
    <li><%= text_field_tag :email, params[:email],:placeholder => "Your Email" %></li>

    <li><%= label_tag :password, "password", :class => "password" %></li>
    <li><%= password_field_tag :password %></li>
    <li><%= submit_tag "Log in", :class => "btn" %></li>
  </ul>
<% end %>
</div>
